// Program to change gray code to binary code..




#include<stdio.h>
int main()
{
	int i,a,n,arr[n];
    printf("Enter the number of digit of Gray code: ");
    scanf("%d",&n);
	printf("Enter Gray code :\n");
	for(i=0;i<n;i++)
	scanf("%d",&arr[i]); 

	for(i=1;i<n;i++)
    {
		//arr[i]=arr[i-1]*!(arr[i]) + !(arr[i-1])*arr[i];
		arr[i]=arr[i-1]^arr[i];
	}
	printf("Binary code is :\n");
	for(i=0;i<n;i++)
	printf("%d",arr[i]);
	return(0);
}
